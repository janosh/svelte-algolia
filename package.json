{
  "name": "svelte-algolia",
  "description": "Algolia server-side index updater and client-side search component for Svelte projects",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://svelte-algolia.netlify.app",
  "repository": "https://github.com/janosh/svelte-algolia",
  "license": "MIT",
  "version": "0.2.13",
  "type": "module",
  "svelte": "Search.svelte",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "svelte-kit build",
    "preview": "svelte-kit preview",
    "package": "svelte-kit package",
    "serve": "yarn build && yarn preview",
    "test": "ava --node-arguments='--experimental-json-modules' --timeout=2m",
    "prepublishOnly": "yarn test"
  },
  "keywords": [
    "svelte",
    "algolia",
    "plugin",
    "search",
    "server-side",
    "client-side"
  ],
  "dependencies": {
    "algoliasearch": "^4.12.0"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^1.0.0-next.26",
    "@sveltejs/kit": "^1.0.0-next.230",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "ava": "^4.0.1",
    "dotenv": "^11.0.0",
    "eslint": "^8.6.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "hastscript": "^7.0.2",
    "mdsvex": "^0.9.8",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.0.1",
    "svelte": "^3.46.1",
    "svelte-check": "^2.3.0",
    "svelte-github-corner": "^0.1.0",
    "svelte-preprocess": "^4.10.1",
    "svelte-toc": "^0.2.2",
    "svelte2tsx": "^0.4.14",
    "typescript": "^4.5.4",
    "vite": "^2.7.12"
  },
  "publishConfig": {
    "access": "public"
  }
}

{
  "name": "svelte-algolia",
  "description": "Algolia server-side index updater and client-side search component for Svelte projects",
  "author": "Janosh Riebesell <janosh.riebesell@gmail.com>",
  "homepage": "https://janosh.github.iosvelte-algolia",
  "repository": "https://github.com/janosh/svelte-algolia",
  "license": "MIT",
  "version": "0.3.2",
  "type": "module",
  "svelte": "./dist/index.js",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "package": "svelte-package",
    "serve": "vite build && vite preview",
    "test": "vitest"
  },
  "keywords": [
    "svelte",
    "algolia",
    "plugin",
    "search",
    "server-side",
    "client-side"
  ],
  "dependencies": {
    "algoliasearch": "^4.14.3",
    "svelte": "^3.55.1"
  },
  "devDependencies": {
    "@sveltejs/adapter-static": "^2.0.1",
    "@sveltejs/kit": "^1.9.2",
    "@sveltejs/package": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^5.54.0",
    "@typescript-eslint/parser": "^5.54.0",
    "dotenv": "^16.0.3",
    "eslint": "^8.35.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "hastscript": "^7.2.0",
    "jsdom": "^21.1.0",
    "mdsvex": "^0.10.6",
    "prettier": "^2.8.4",
    "prettier-plugin-svelte": "^2.9.0",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-slug": "^5.1.0",
    "svelte-check": "^3.0.4",
    "svelte-preprocess": "^5.0.1",
    "svelte-toc": "^0.5.2",
    "svelte-zoo": "^0.3.4",
    "svelte2tsx": "^0.6.2",
    "typescript": "^4.9.5",
    "vite": "^4.1.4",
    "vitest": "^0.29.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "exports": {
    "./Search.svelte": {
      "types": "./dist/Search.svelte.d.ts",
      "svelte": "./dist/Search.svelte",
      "default": "./dist/Search.svelte"
    },
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./server-side": {
      "types": "./dist/server-side.d.ts",
      "default": "./dist/server-side.js"
    }
  },
  "files": [
    "dist"
  ]
}
